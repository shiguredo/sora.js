{
  "name": "sora-js-sdk",
  "version": "2024.2.0-canary.14",
  "description": "WebRTC SFU Sora JavaScript SDK",
  "main": "dist/sora.js",
  "module": "dist/sora.mjs",
  "types": "./dist/sora.d.ts",
  "exports": {
    ".": {
      "import": "./dist/sora.mjs",
      "require": "./dist/sora.mjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "examples": "vite build",
    "e2e-test": "pnpm run build && playwright test --project=chromium",
    "build": "pnpm -r --filter=./packages/* build",
    "lint": "pnpm -r --filter=./packages/* --filter=./examples lint",
    "fmt": "pnpm -r --filter=./packages/* --filter=./examples fmt",
    "test": "pnpm -r --filter=./packages/* test",
    "check": "pnpm -r --filter=./packages/* --filter=./examples check",
    "doc": "typedoc",
    "release:canary": "./scripts/release_canary.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shiguredo/sora-js-sdk.git"
  },
  "author": "Shiguredo Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://discord.gg/shiguredo"
  },
  "homepage": "https://github.com/shiguredo/sora-js-sdk#readme",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@playwright/test": "1.49.0",
    "typedoc": "0.27.1",
    "typescript": "5.7.2",
    "vite": "6.0.1",
    "vitest": "2.1.6"
  },
  "packageManager": "pnpm@9.14.3",
  "engines": {
    "node": ">=18"
  }
}
